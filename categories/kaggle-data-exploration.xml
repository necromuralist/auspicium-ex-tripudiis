&lt;div class="highlight"&gt;&lt;pre&gt;&lt;span&gt;&lt;/span&gt;super_set[DataKeys.item_category] = super_set.item_id.apply(
    lambda item: frames[DataSource.items].loc[item, DataKeys.item_category])
&lt;/pre&gt;&lt;/div&gt;

